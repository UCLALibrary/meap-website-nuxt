    "vue-router": "^4.4.3",
    "vuetify": "^3.7.4"
  },
  "devDependencies": {
    "@chromatic-com/cypress": "^0.11.8",
    "@nuxt/eslint": "1.15.1",
    "@nuxt/scripts": "^0.13.2",
    "@nuxtjs/sitemap": "^5.3.5",
    "@percy/cli": "^1.31.1",
    "@percy/cypress": "^3.1.2",
    "@types/node": "^18.17.3",
    "@ucla-library/component-library-nuxt-module": "1.3.0",
    "chromatic": "^15.1.0",
    "cross-env": "^10.1.0",
    "cypress": "^15.0.0",
    "date-fns": "^3.6.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-cypress": "^2.15.1",
    "html-to-text": "^9.0.5",
    "lodash": "^4.17.21",
    "nuxt": "4.3.1",
    "nuxt-graphql-request": "^8.1.1",
    "sass": "^1.66.1",
    "typescript": "^5.9.3",
    "ucla-library-design-tokens": "^5.55.0",
    "vite-svg-loader": "^5.1.0"
  }
